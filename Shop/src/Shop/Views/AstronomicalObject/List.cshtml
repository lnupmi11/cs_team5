<div>
    <h1> Our astronomical objects </h1>
</div>
@{
    foreach (var astronomicalObject in Model)
    {
        <div>
            <h2 style="color:blue">Astronomical Object </h2>
            <p>Name : @astronomicalObject.Name</p>
            <p>Price : @astronomicalObject.Price.ToString("")</p>
            <p>Description: @astronomicalObject.ShortDescription</p>
        </div>

    }
}